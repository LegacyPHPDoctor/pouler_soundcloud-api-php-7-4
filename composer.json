{
    "name": "legacyphpdoctor/pouler_soundcloud-api-php-7-4",
    "description": "PHP Wrapper for the SoundCloud API",
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Radt",
            "email": "paulradt@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "keywords": ["SoundCloud", "api"],
    "homepage": "https://github.com/PouleR/soundcloud-api",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "require": {
        "php": "^7.4 || >=8.1",
        "ext-json": "*",
        "symfony/http-client": "^5.4",
        "symfony/mime": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "PouleR\\SoundCloudAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PouleR\\SoundCloudAPI\\Tests\\": "tests/"
        }
    }
}
